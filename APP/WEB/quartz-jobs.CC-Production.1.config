      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>read-alerts-from-c6-for-web-watch-trigger</name>
        <job-name>read-alerts-from-c6-for-web-watch</job-name>
        <!--6:00am everyday-->
